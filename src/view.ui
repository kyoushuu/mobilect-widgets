<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.8 -->
  <object class="GdHeaderSimpleButton" id="button_new">
    <property name="visible">True</property>
    <property name="halign">start</property>
    <property name="label">New</property>
    <signal name="clicked" handler="mpcw_view_on_button_new_clicked" swapped="no"/>
  </object>
  <object class="GtkBox" id="box_select">
    <property name="visible">True</property>
    <child>
      <object class="GdHeaderToggleButton" id="togglebutton_select">
        <property name="visible">True</property>
        <property name="halign">end</property>
        <property name="label">Select Items</property>
        <property name="symbolic-icon-name">object-select-symbolic</property>
        <signal name="toggled" handler="mpcw_view_on_togglebutton_select_toggled" swapped="no"/>
      </object>
    </child>
    <child>
      <object class="GdHeaderToggleButton" id="togglebutton_done">
        <property name="visible">True</property>
        <property name="halign">end</property>
        <property name="label">Done</property>
        <style>
          <class name="suggested-action"/>
        </style>
      </object>
    </child>
  </object>
  <object class="GtkBox" id="box">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkOverlay" id="overlay">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <child>
          <object class="GtkScrolledWindow" id="scrolledwindow">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <child>
              <object class="GtkTreeView" id="treeview">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="headers_visible">False</property>
                <property name="reorderable">True</property>
                <property name="enable_search">False</property>
                <property name="activate_on_single_click">True</property>
                <style>
                  <class name="content-view"/>
                </style>
                <signal name="row-activated" handler="mpcw_view_on_treeview_row_activated" swapped="no"/>
                <child internal-child="selection">
                  <object class="GtkTreeSelection" id="treeview-selection_results">
                    <property name="mode">none</property>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="treeviewcolumn_selected">
                    <property name="min_width">50</property>
                    <property name="title" translatable="yes">Selected</property>
                    <property name="alignment">0.5</property>
                    <child>
                      <object class="GtkCellRendererToggle" id="cellrenderertoggle_selected"/>
                      <attributes>
                        <attribute name="active">0</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
      <packing>
        <property name="expand">True</property>
        <property name="fill">True</property>
        <property name="position">0</property>
      </packing>
    </child>
  </object>
</interface>
