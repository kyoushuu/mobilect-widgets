<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.6 -->
  <object class="GtkBox" id="box">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="hexpand">True</property>
    <property name="vexpand">True</property>
    <property name="orientation">vertical</property>
    <child>
      <object class="GdHeaderBar" id="headerbar">
        <property name="visible">True</property>
        <child>
          <object class="GdHeaderSimpleButton" id="button_back">
            <property name="halign">start</property>
            <property name="label">Back</property>
            <property name="symbolic-icon-name">go-previous-symbolic</property>
            <signal name="clicked" handler="mpcw_window_on_button_back_clicked" swapped="no"/>
          </object>
          <packing>
            <property name="pack_type">start</property>
          </packing>
        </child>
      </object>
    </child>
    <child>
      <object class="MpcwStack" id="stack">
        <property name="visible">True</property>
        <property name="hexpand">True</property>
        <property name="vexpand">True</property>
        <signal name="add" handler="mpcw_window_on_stack_add_remove" swapped="no"/>
        <signal name="remove" handler="mpcw_window_on_stack_add_remove" swapped="no"/>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="fill">True</property>
      </packing>
    </child>
  </object>
</interface>
